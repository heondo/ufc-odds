{
  "name": "ufc-odds",
  "version": "0.0.1",
  "description": "UseContext, data storage, Node/Express",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "webpack --mode=production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon src/server/index.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@trendmicro/react-dropdown": "^1.3.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cron": "^1.7.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formik": "^2.0.6",
    "immutability-helper": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.4",
    "pg": "^7.14.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dnd": "^9.5.1",
    "react-dnd-html5-backend": "^9.5.1",
    "react-dom": "16.8.6",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.10.1",
    "styled-components": "^4.4.1",
    "universal-cookie": "^4.0.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.6",
    "css-loader": "^3.2.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "4.0.0",
    "lint-staged": "8.1.6",
    "npm-run-all": "4.1.5",
    "style-loader": "^1.0.0",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1"
  },
  "eslintIgnore": [
    "/public/main.js"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": [
      "plugin:react/recommended",
      "eslint:recommended",
      "standard"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "padded-blocks": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "curly": [
        "error",
        "multi-line"
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0
    },
    "plugins": [
      "react-hooks"
    ]
  },
  "keywords": [],
  "author": "heondo",
  "license": "ISC"
}
